# Test environment configuration
ENVIRONMENT=test
LOG_LEVEL=ERROR

# API Configuration
API_HOST=localhost
API_PORT=8000

# Database
DATABASE_URL=sqlite:///:memory:
REDIS_URL=redis://localhost:6379

# Trading Configuration
MAX_POSITION_SIZE_PCT=0.10
MAX_LEVERAGE=3.0
STOP_LOSS_PCT=0.05
TAKE_PROFIT_PCT=0.15
