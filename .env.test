# OpenRouter API
OPENROUTER_API_KEY=test_key
OPENROUTER_MODEL=openai/gpt-4

# Exchange Configuration
EXCHANGE_ID=binance
EXCHANGE_API_KEY=test_key
EXCHANGE_SECRET=test_secret
EXCHANGE_TESTNET=true

# Monitoring
ENABLE_METRICS=false
ENABLE_MONITORING=false
